{
	"window.zoomLevel": 2,
	"workbench.editor.enablePreview": false,
	"workbench.colorTheme": "Community Material Theme",
	"editor.formatOnSave": true,
	"files.autoSave": "onFocusChange",
	"terminal.integrated.fontFamily": "Hack Nerd Font",
	"workbench.startupEditor": "none",
	"javascript.updateImportsOnFileMove.enabled": "always",
	"git.autofetch": true,
	"terraform.format": {},
	"colorize.colorize_only_visible_beta": true,
	"colorize.colorized_colors": ["BROWSERS_COLORS", "HEXA", "RGB", "HSL"],
	"colorize.colorized_variables": ["SASS", "CSS", "LESS", "STYLUS"],
	"colorize.exclude": [
		"**/.git",
		"**/.svn",
		"**/.hg",
		"**/CVS",
		"**/.DS_Store",
		"**/.git",
		"**/node_modules",
		"**/bower_components",
		"**/tmp",
		"**/dist",
		"**/tests"
	],
	"colorize.languages": [
		"css",
		"sass",
		"scss",
		"less",
		"postcss",
		"sss",
		"stylus",
		"xml",
		"svg",
		"javascript"
	],
	"bracket-pair-colorizer-2.excludedLanguages": [],
	"files.insertFinalNewline": true,
	"terraform.indexing": {
		"enabled": false,
		"liveIndexing": false,
		"delay": 500,
		"exclude": [".terraform/**/*", "**/.terraform/**/*"]
	},
	"terraform.languageServer": {
		"external": false,
		"args": ["serve"]
	},
	"todo-tree.tree.showScanModeButton": false,
	"go.useLanguageServer": true,
	"markdown.extension.toc.levels": "2..6",
	"markdown.extension.katex.macros": {},
	"markdownlint.config": {
		"MD041": false,
		"MD033": false,
		"MD034": false
	},
	"vim.vimrc.enable": true,
	"vim.vimrc.path": "/Users/soerenmartius/.vimrc",
	"workbench.activityBar.visible": true,
	"markdown.trace": "verbose",
	"prettier.disableLanguages": ["markdown"],
	"markdown.extension.toc.omittedFromToc": {},
	"editor.wordWrap": "bounded",
	"prettier.printWidth": 80,
	"vim.textwidth": 80,
	"editor.wrappingIndent": "same",
	"editor.wordWrapColumn": 120,
	"editor.rulers": [80, 120],
	"editor.suggestSelection": "first",
	"vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
	"cSpell.enableFiletypes": ["html", "markdown", "!javascript"],
	"cSpell.userWords": [
		"Acess",
		"Alexa",
		"CNAME",
		"CODEOWNERS",
		"Gwfn",
		"Mineiros",
		"NAPTR",
		"Policiy",
		"SIGQUIT",
		"Signup",
		"TESTACCOUNT",
		"Terratest",
		"YOURGPGKEYID",
		"boostrap",
		"downlevel",
		"entrypoint",
		"esnext",
		"fullhuman",
		"geolocation",
		"gofmt",
		"goimports",
		"golint",
		"hashicorp",
		"hashids",
		"healthcheck",
		"jrcs",
		"jsonencode",
		"justredirect",
		"keyval",
		"letsencrypt",
		"mariux",
		"milli",
		"packpath",
		"purgecss",
		"repos",
		"runtimepath",
		"runtimes",
		"schmunk",
		"scripthost",
		"signin",
		"soerenmartius",
		"sourcemap",
		"stretchr",
		"tailwindcss",
		"tbkcnsonk",
		"tflint",
		"thenaturalist",
		"tslib",
		"typechecking",
		"updatefound",
		"vhost",
		"vimrc",
		"vuex"
	],
	"workbench.editor.showTabs": true,
	"editor.wordSeparators": "`~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?=-=-",
	"[typescript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[javascript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"vetur.format.defaultFormatter.js": "prettier-eslint",
	"emmet.extensionsPath": "",
	"vetur.experimental.templateInterpolationService": true,
	"[vue]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"eslint.format.enable": true
}
